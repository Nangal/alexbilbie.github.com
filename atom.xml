---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Alex Bilbie</title>
 <link href="http://alexbilbie.com/atom.xml" rel="self"/>
 <link href="http://alexbilbie.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://alexbilbie.com</id>
 <author>
   <name>Alex Bilbie</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://alexbilbie.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://alexbilbie.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>